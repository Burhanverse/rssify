{
  "type": "module",
  "dependencies": {
    "@types/xml2js": "^0.4.14",
    "axios": "^1.7.7",
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.5",
    "feedparser": "^2.2.10",
    "grammy": "^1.34.0",
    "markdown-escape": "^2.0.0",
    "mongodb": "^6.13.0",
    "p-limit": "^6.2.0",
    "pretty-bytes": "^7.0.0",
